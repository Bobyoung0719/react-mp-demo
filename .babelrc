{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }], 
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": {"version": 3, "proposals": true},
      "useESModules": true
    }],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ]
}
